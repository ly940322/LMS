    </view-entity>


    <!-- 取得仓库信息列表 -->
    <view-entity entity-name="GetFacilityList" package-name="org.ofbiz.product.product"
                 title="Get facilityList For ProductId View Entity">
        <member-entity entity-alias="PF" entity-name="ProductFacility"/>
