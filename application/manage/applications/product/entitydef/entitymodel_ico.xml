        <view-link entity-alias="PPC" rel-entity-alias="PG">
            <key-map field-name="partyId"/>
        </view-link>
    </view-entity>
    <!-- 根据商家取得商品分类信息 End by zhajh 2018/05/22  -->


    <!-- 会员收藏 -->
